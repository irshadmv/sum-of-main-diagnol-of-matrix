#include <stdio.h>
int main() {
    int a[20][20],m,n,i,j,sum=0;
    printf("enter the row and column");
    scanf("%d%d",&m,&n);
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("enter the number");
            scanf("%d",&a[i][j]);
        }
    }
     for(i=0;i<m;i++)
    
        {
            sum=sum+a[i][i];
        }
        
    printf("%d",sum);
    return 0;
}
